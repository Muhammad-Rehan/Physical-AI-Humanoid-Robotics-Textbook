# Google API Configuration
GOOGLE_GEMINI_API_KEY=your_google_gemini_api_key_here

# Qwen API Configuration
QWEN_API_KEY=your_qwen_api_key_here

# Qdrant Configuration
QDRANT_URL=your_qdrant_url_here
QDRANT_API_KEY=your_qdrant_api_key_here

# Embedding Configuration
# Set to "true" to use local embeddings instead of Google embeddings
# This is useful when Google API quota is exceeded or for offline development
USE_LOCAL_EMBEDDINGS=false

# Set to "true" to use Qwen embeddings instead of Google embeddings
# This is useful when you prefer Qwen's embedding model
USE_QWEN_EMBEDDINGS=false

# Generation Configuration
# Set to "true" to use Qwen for text generation instead of Google Gemini
# This is useful when you prefer Qwen's language model
USE_QWEN_GENERATION=false

# Optional: Specify which local model to use (default is "all-MiniLM-L6-v2")
# LOCAL_EMBEDDING_MODEL=all-MiniLM-L6-v2